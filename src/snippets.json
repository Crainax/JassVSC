  "if-zinc": {
    "prefix": "if",
    "body": "if (${1:condition}) {${0}}",
    "description": "if-zinc",
    "scope": "source.jass"
  },
  "elseif-zinc": {
    "prefix": "elseif",
    "body": "else if (${1:condition}) {${0}}",
    "description": "elseif-zinc",
    "scope": "source.jass"
  },
  "for": {
    "prefix": "for",
    "body": "integer ${1};\nfor (${2:0} <= ${1:index} <= ${3:10}) {${0:}}",
    "description": "for循环",
    "scope": "source.jass"
  },
  "while": {
    "prefix": "while",
    "body": "while (${1:true}) {${0:}}",
    "description": "while循环",
    "scope": "source.jass"
  },
  "block-zinc": {
    "prefix": "zinc",
    "body": "//! zinc\n  ${0:}\n//! endzinc",
    "description": "zinc-block",
    "scope": "source.jass"
  },
  "interface-zinc": {
    "prefix": "interface",
    "body": "interface ${1:interface_name} {\n${0:}\n}",
    "description": "zinc接口",
    "scope": "source.jass"
  },
  "struct-zinc": {
    "prefix": "struct",
    "body": "${1:public }struct ${2:struct_name} ${3:[]} {\n${0:}\n}",
    "description": "zinc struct 模板",
    "scope": "source.jass"
  },
  "module-zinc": {
    "prefix": "module",
    "body": "public module ${1:module_name} {\n${0:}\n}",
    "description": "zinc module 模板",
    "scope": "source.jass"
  },
  "function-zinc": {
    "prefix": "function",
    "body": "${1:public }function ${2:function_name} (${3})${5:  -> ${4:nothing}} {${0:}}",
    "description": "zinc function 模板",
    "scope": "source.jass"
  },
  "method-zinc": {
    "prefix": "method",
    "body": "${1:public }${2:static }method ${3:function_name} (${4})${6:  -> ${5:nothing}} {${0:}}",
    "description": "zinc method 模板",
    "scope": "source.jass"
  },
  "public-block": {
    "prefix": "public",
    "body": "public {\n${0:}\n}",
    "description": "zinc public 模块",
    "scope": "source.jass"
  },
  "private-block": {
    "prefix": "private",
    "body": "private {\n${0:}\n}",
    "description": "zinc private 模块",
    "scope": "source.jass"
  },
  "Print-zinc": {
    "prefix": "sout",
    "body": "BJDebugMsg(\"${1}\");",
    "description": "调用debug打印信息",
    "scope": "source.jass"
  },
  // 这里是两者共用
  "长注释": {
    "prefix": "CMM",
    "body": "//-----------------------------",
    "description": "横线分割的注释",
    "scope": "source.jass"
  },
  "textmacro": {
    "prefix": "textmacro",
    "body": "//! textmacro textmacro_name takes args\n\t${0:\\$args\\$}\n//! endtextmacro",
    "description": "文本宏",
    "scope": "source.jass"
  },
  "runtextmacro": {
    "prefix": "runtextmacro",
    "body": "//! runtextmacro ${1:TextMacroName}(\"${2:1}\")",
    "description": "运行文本宏",
    "scope": "source.jass"
  },
